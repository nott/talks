\ProvidesPackage{beamercolorthemeWG}
[2013/07/22 Wargaming beamer color theme]

\mode<presentation>

\definecolor{WGblack}{RGB}{0,0,0}
\definecolor{WGwhite}{RGB}{255,255,255}
\definecolor{WGred}{RGB}{255,0,0}

\setbeamercolor{title}{fg=WGwhite}
\setbeamercolor{author}{fg=WGwhite}
\setbeamercolor{date}{fg=WGwhite}
\setbeamercolor{normal text}{fg=WGblack}
\setbeamercolor{frametitle}{fg=WGblack}
\setbeamercolor{item}{fg=WGred}
\setbeamercolor{block title}{fg=WGred}
\setbeamercolor{section in toc}{fg=WGred}
\setbeamercolor{footline}{fg=WGwhite}
\setbeamercolor{title in head/foot}{fg=WGwhite,bg=WGblack}


\mode<all>
